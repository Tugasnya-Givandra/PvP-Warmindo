<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BurjoHolic7"
        tools:targetApi="31">
        <activity
            android:name=".ui.transaction_details.DetailMakananAdapter"
            android:exported="false" />
        <activity
            android:name=".ui.transaction_details.DetailTransactionPage"
            android:label="@string/title_detail_transaksi"
            android:exported="true" />
        <activity
            android:name=".Detail_transaction"
            android:exported="false" />
        <activity
            android:name=".TransactionActivity"
            android:exported="true"
            android:label="@string/title_activity_transaction" />
        <activity
            android:name=".Halaman_transaksi"
            android:exported="false" />
        <activity
            android:name=".transaction_page"
            android:exported="false"
            android:label="@string/title_activity_transaction_page"
            android:theme="@style/Theme.BurjoHolic7.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>